{
    "manifest_version": 3,
    "name": "AniTrace",
    "description": "Track your favorite Animes all in one place",
    "version": "1.0",
    "action": {
      "default_popup": "popup.html",
      "default_icon": "no.png"
    },
    "permissions":[
      "storage"
    ],
    "host_permissions": [
      "https://*/*"
    ]
}